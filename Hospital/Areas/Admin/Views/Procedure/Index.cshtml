@model List<Procedure>

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>id</th>

            <th>Name</th>
            <th>ProcessTime</th>
            <th>Actions</th>






        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Id

                </td>
                <td>
                    @item.Name

                </td>
                <td>
                    @item.ProcessTime

                </td>
                

                <td>
                    <a asp-action="Update" asp-controller="Procedure" asp-route-id="@item.Id" class="btn btn-success">Update</a>
                    <a asp-action="Delete" asp-route-id="@item.Id" asp-controller="Procedure" class="btn btn-danger">Delete</a>
                    <a  asp-action="AddDoctor" class="btn btn-warning" asp-controller="Procedure" asp-route-id="@item.Id">Add Doctor</a>
                </td>
            </tr>
        }
    </tbody>
</table>

