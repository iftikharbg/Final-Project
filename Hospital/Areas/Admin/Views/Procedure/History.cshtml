
@model HistoryViewModel

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>id</th>


          
            <th>PacientName</th>
            <th>Procedure</th>
            <th>Price</th>
            <th>Doctor</th>
            <th>RezervationStartTime</th>
            <th>RezervationEndTime</th>




        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Rezervations)
        {
        <tr>
            <td>
                @item.Id

            </td>
            <td>
                @item.Pasient.Name

            </td>
            <td>
                @item.Procedure.Name

            </td>
            <td>
                @item.Price

            </td>
            <td>
               @item.Doctor.Name
            </td>
            <td>
                @item.StartDate

            </td>

            <td>
                @item.StartDate.AddMinutes(item.Procedure.ProcessTime)

            </td>


            
        </tr>
        }
    </tbody>
</table>


